{
  "name": "character-prefix-completion",
  "displayName": "Character Prefix Code Completion",
  "description": "Code completion with character prefix conditioning using Project Chimera",
  "version": "0.1.0",
  "publisher": "chimera-project",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:characterPrefix.completeWithPrefix",
    "onCommand:characterPrefix.switchModelType"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "characterPrefix.completeWithPrefix",
        "title": "Character Prefix: Complete with Prefix"
      },
      {
        "command": "characterPrefix.switchModelType",
        "title": "Character Prefix: Switch Model Type"
      }
    ],
    "configuration": {
      "title": "Character Prefix Completion",
      "properties": {
        "characterPrefix.apiUrl": {
          "type": "string",
          "default": "http://localhost:8000",
          "description": "URL of the Chimera API server"
        },
        "characterPrefix.apiKey": {
          "type": "string",
          "default": "",
          "description": "API key for authentication with the Chimera API"
        },
        "characterPrefix.modelType": {
          "type": "string",
          "enum": [
            "openai",
            "gemini"
          ],
          "default": "openai",
          "description": "Type of model to use for character prefix conditioning"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.60.0",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "code completion",
    "ai",
    "character prefix",
    "openai",
    "gemini",
    "project chimera"
  ],
  "author": "Project Chimera Team",
  "license": "MIT"
}
